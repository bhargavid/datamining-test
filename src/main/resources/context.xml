<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:context="http://www.springframework.org/schema/context"
             xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd
        ">

    <context:annotation-config/>

    <beans:bean id="jobDriver"
                class="com.mstest.datamining.app.JobDriver">
    </beans:bean>

    <beans:bean id="dtService"
                class="com.mstest.datamining.service.DecisionTreeServiceImpl">
    </beans:bean>

    <beans:bean id="mlpService"
                class="com.mstest.datamining.service.MLPServiceImpl">
    </beans:bean>

    <beans:bean id="knnService"
                class="com.mstest.datamining.service.KnnServiceImpl">
    </beans:bean>

    <beans:bean id="adaBoostService"
                class="com.mstest.datamining.service.AdaBoostServiceImpl">
    </beans:bean>
</beans:beans>
